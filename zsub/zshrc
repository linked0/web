tisr /tigger-bridge-server/

pp package.json
hh hardhat.config.ts
ee .env
rr README.md
fo fountry.toml
re remappings.txt
ma Makefile
"
}

cc() {
  local folder=""
  local file=""
  
  if [ "$1" = "69" ]; then
    folder="$HOME/work/webf/erc6900-reference/"
  elif [ "$1" = "43" ]; then
    folder="$HOME/work/web/erc4337-aa/"
  elif [ "$1" = "webf" ]; then
    folder="$HOME/work/webf/"
  elif [ "$1" = "fo" ]; then
    folder="$HOME/work/webf/course-foundry/L7-foundry-fund-me/"
  elif [ "$1" = "ex" ]; then
    folder="$HOME/work/web/ex/"
  elif [ "$1" = "ex5" ]; then
    folder="$HOME/work/web/ex5/"
  elif [ "$1" = "exf" ]; then
    folder="$HOME/work/webf/exf/"
  elif [ "$1" = "exj" ]; then
    folder="$HOME/work/web/ex-j/"
  elif [ "$1" = "exp" ]; then
    folder="$HOME/work/web/exp/"
  elif [ "$1" = "exw" ]; then
    folder="$HOME/work/web/ex-w/"
  elif [ "$1" = "oz" ]; then
    folder="$HOME/work/openzeppelin/"
  elif [ "$1" = "ha" ]; then
    folder="$HOME/work/smart-contract-hacking/"
  elif [ "$1" = "poco" ]; then
    folder="$HOME/work/pooh-land-contract/"
  elif [ "$1" = "posr" ]; then
    folder="$HOME/work/pooh-land-backend/"
  elif [ "$1" = "powe" ]; then
    folder="$HOME/work/pooh-land-frontend/"
  elif [ "$1" = "rajs" ]; then
    folder="$HOME/work/rabbit-js/"
  elif [ "$1" = "rasr" ]; then
    folder="$HOME/work/rabbit-server/"
  elif [ "$1" = "rawe" ]; then
    folder="$HOME/work/rabbit-web/"
  elif [ "$1" = "rawa" ]; then
    folder="$HOME/work/rabbit-wallet/"
  elif [ "$1" = "tisw" ]; then
    folder="$HOME/work/tigger-swap-contract/"
  elif [ "$1" = "tico" ]; then
    folder="$HOME/work/tigger-bridge-contract/"
  elif [ "$1" = "tisr" ]; then
    folder="$HOME/work/tigger-bridge-server/"
  elif [ -z "$2" ]; then
    code $@
    return
  fi
  
  if [ "$2" = "pp" ]; then
    file="package.json"
  elif [ "$2" = "hh" ]; then
    file="hardhat.config.ts"
  elif [ "$2" = "ee" ]; then
    file=".env"
  elif [ "$2" = "rr" ]; then
    file="README.md"
  elif [ "$2" = "fo" ]; then
    file="foundry.toml"
  elif [ "$2" = "re" ]; then
    file="remappings.txt"
  elif [ "$2" = "ma" ]; then
    file="Makefile"
  fi

  echo "code \"$folder$file\""
  code "$folder$file"
}

dd() {
  local folder=""
  local file=""
  
  if [ "$1" = "69" ]; then
    folder="$HOME/work/webf/erc6900-reference/"
  elif [ "$1" = "43" ]; then
    folder="$HOME/work/web/erc4337-aa/"
  elif [ "$1" = "webf" ]; then
    folder="$HOME/work/webf/"
  elif [ "$1" = "fo" ]; then
    folder="$HOME/work/web/ecourse-foundry/L7-foundry-fund-me/"
  elif [ "$1" = "ex" ]; then
    folder="$HOME/work/web/ex/"
  elif [ "$1" = "exf" ]; then
    folder="$HOME/work/web/exf/"
  elif [ "$1" = "exj" ]; then
    folder="$HOME/work/web/ex-j/"
  elif [ "$1" = "exp" ]; then
    folder="$HOME/work/web/exp/"
  elif [ "$1" = "exw" ]; then
    folder="$HOME/work/web/ex-w/"
  elif [ "$1" = "oz" ]; then
    folder="$HOME/work/openzeppelin/"
  elif [ "$1" = "ha" ]; then
    folder="$HOME/work/smart-contract-hacking/"
  elif [ -z "$2" ]; then
    code $@
    return
  fi

  echo "cd \"$folder\""
  cd "$folder"
}


