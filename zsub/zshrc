#######################################
### Below is the common aliases

hgrep() {
	grep -irnH --include=\*.$1 "$2" ./
}
ii() {
  idea $@
}
adf() {
  echo "echo \"alias $1='code $2'\" >> ~/.zshrc"
}
ch() {
  chatgpt $@
}
chc() {
  cat $1 | ch $2
}
chm() {
  ch "$(cat ~/work/web/m2.txt)"
}
chcm() {
  cat $@ | ch "$(cat ~/work/web/m2.txt)"
}
cmm() {
  if [ -z "$1" ]; then
    echo "No argument"
    return
  fi
  code ~/work/web/zsub/zzmemo/$1
}
py() {
  if [ -z "$1" ]; then
    echo "No argument"
    echo "Usage: py 123/34"
    return
  fi
   python -c "print($1)"
}
py2() {
  if [ -z "$1" ]; then
    echo "No argument"
    echo "Usage: py2 \"a=2;b=2;print(a+b);\""
    return
  fi
   python -c "$1"
}

ft() {
  forge test -vvv --match-test $1
}
ftc() {
  forge test -vvv --match-contract $1
}
ftf() {
  forge test -vvv --match-path $1
}
ft2() {
  forge test -vvvv --match-test $1
}
ftc2() {
  forge test -vvvv --match-contract $1
}
ftf2() {
  forge test -vvvv --match-path $1
}

# Declare an associative array
declare -A folderMap
declare -A fileMap

# Assign values to the map
fileMap["aaa"]="$HOME/work/web/contracts/AllBasic.sol"
fileMap["bbb"]="$HOME/work/web/test/all-basic.ts"
fileMap["ccc"]="$HOME/work/web/scripts/deploy.ts"
fileMap["aaaf"]="$HOME/work/webf/src/Counter.sol"
fileMap["bbbf"]="$HOME/work/webf/test/Counter.t.sol"
fileMap["cccf"]="$HOME/work/webf/script/Counter.s.sol"
fileMap["ddd"]="$HOME/work/web/dev.md"
fileMap["eee"]="$HOME/work/web/.env"
fileMap["ppp"]="$HOME/work/web/package.json"
fileMap["hhh"]="$HOME/work/web/hardhat.config.ts"
fileMap["ttt"]="$HOME/work/web/tsconfig.json"
fileMap["rrr"]="$HOME/work/web/README.md"
fileMap["mmm"]="$HOME/work/web/memo.txt"
fileMap["mm2"]="$HOME/work/web/m2.txt"
fileMap["mms"]="$HOME/work/web/ms.txt"
fileMap["zz"]="$HOME/.zshrc"
fileMap["zz2"]="$HOME/work/web/zsub/zshrc"
fileMap["ff"]="./foundry.toml"
fileMap["pp"]="./package.json"
fileMap["hh"]="./hardhat.config.ts"
fileMap["ee"]="./.env"
fileMap["re"]="./remappings.txt"
fileMap["rr"]="./README.md"
fileMap["ma"]="./Makefile"

ii() {
  local file=""
  file=${fileMap["$1"]}

  echo "idea $file"
  idea "$file"
}

cc() {
  local file=""
  file=${fileMap["$1"]}

  echo "cat $file"
  cat "$file"
}

