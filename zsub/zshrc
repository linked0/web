fileMap["pp"]="package.json"
fileMap["hh"]="hardhat.config.ts"
fileMap["ee"]=".env"
fileMap["rr"]="README.md"
fileMap["fo"]="fountry.toml"
fileMap["re"]="eemappings.txt"
fileMap["ma"]="Makefile"

cc() {
  local folder=""
  local file=""

  folder=${folderMap["$1"]}
  if [ -z "$1" ]; then
    for key in "${(@k)folderMap}"; do
      echo "Key: $key, Value: ${folderMap[$key]}"
    done
  fi

  if [ -z "$2" ]; then
    if [ -z "$folder" ]; then
      code $@
    else
      echo "cd \"$folder\""
      cd "$folder"
    fi
    return
  fi

  if [ -z "$folder" ]; then
    folder="./"
  fi
  
  file=${fileMap["$2"]}
  if [ -z "$file" ]; then
    file=$2
  fi

  echo "code \"$folder$file\""
  code "$folder$file"
}

