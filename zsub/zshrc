#######################################
### Below is the common aliases

hgrep() {
	grep -irnH --include=\*.$1 "$2" ./
}
ii() {
  idea $@
}
ch() {
  chatgpt $@
}
chc() {
  cat $1 | ch $2
}
chm() {
  ch "$(cat ~/work/web/m2.txt)"
}
chcm() {
  cat $@ | ch "$(cat ~/work/web/m2.txt)"
}
adf() {
  echo "echo \"alias $1='code $2'\" >> ~/.zshrc"
}

ft() {
  forge test -vvv --match-test $1
}
ftv() {
  forge test -vvvv --match-test $1
}

ccp() {
  echo "
