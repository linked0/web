{
    "name": "validators",
    "version": "0.0.0",
    "description": "API server for Agora",
    "main": "",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "clean": "node_modules/.bin/hardhat clean",
        "build": "node_modules/.bin/hardhat compile",
        "start:testnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network testnet",
        "start:mainnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network mainnet",
        "start:votera": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network votera",
        "start:hardhat": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network hardhat",
        "test": "NODE_ENV=test node_modules/.bin/hardhat test",
        "test:endpoint": "NODE_ENV=test node_modules/.bin/hardhat test test/server/Endpoints.test.ts",
        "test:config": "NODE_ENV=test node_modules/.bin/hardhat test test/service/Config.test.ts",
        "lint": "tslint --config tslint.json \"{src,test}/**/*.ts\"",
        "lint-fix": "tslint --fix --config tslint.json \"{src,test}/**/*.ts\"",
        "prettier": "node_modules/.bin/prettier --write --config .prettierrc 'src/**/*.ts' 'test/**/*.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zeroone-boa/validators.git"
    },
    "author": "BOSAGORA Foundation",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zeroone-boa/validators/issues"
    },
    "homepage": "https://github.com/zeroone-boa/validators#readme",
